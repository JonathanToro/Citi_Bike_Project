    "\n",
    "year = \"2015\"\n",
    "month = [\"01\",'02','03','04','05','06','07','08','09','10','11','12']\n",
    "name = \"-citibike-tripdata.zip\"\n",
    "link =  'https://s3.amazonaws.com/tripdata/'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "##Download data from S3 storage\n",
    "for i in month:\n",
    "    wget.download(link+year+i+name)"
   ]
  }
